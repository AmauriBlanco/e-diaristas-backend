<div class='content-header'>
  <div class='container-fluid'>
    <h1>Lista de Usuários</h1>
  </div>
</div>
<div class='content'>
  <div class='container-fluid'>
    <table class='table'>
      <thead>
        <tr>
          <th scope='col'>ID</th>
          <th scope='col'>Nome</th>
          <th scope='col'>E-mail</th>
          <th scope='col'>Ações</th>
        </tr>
      </thead>
      <tbody>
        {{!-- {{#each usuario}} --}}
        <tr>
          <td>{{id}}</td>
          <td>{{nome}}</td>
          <td>{{email}}</td>
          <td>
            <a
              href='/admin/servicos/{{this.id}}/edit'
              class='btn btn-primary'
            >Atualizar</a>
          </td>
          <td>
            <a
              href='/admin/servicos/{{this.id}}/edit'
              class='btn btn-danger'
            >Excluir</a>
          </td>
        </tr>
        {{!-- {{/each}} --}}
      </tbody>
    </table>
    <div class='float-left'>
      <a href='/admin/servicos/create' class='btn btn-success'>Novo Usuário</a>
    </div>
  </div>
</div>